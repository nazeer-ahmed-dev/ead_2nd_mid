<%- include('header') -%>

<div class="text-center center" style="margin-top: 50px;"><h2>Register</h2></div>
<p></p>

<div class="container  border py-5 text-center">
    <div class="d-flex justify-content-start">
        <div class="card" style="width: 17rem; height:15rem ;">
            <img class="card-img-top" src="..." alt="Card image cap">
            
          </div>
    </div>

    <div class="d-flex justify-content-end">
    <form action="/add" method="post" enctype="multipart/form-data" >
        
        <div class="mb-3 ">
            <!-- <label for="email1" class="form-label">Email</label> -->
            <input type="file" name="image"/>
        </div>
        <div class="mb-3 ">
            <!-- <label for="email1" class="form-label">Email</label> -->
            <input class="form-control" type="text" name="name" id="email1" placeholder="Name"/>
        </div>
        <div class="mb-3 ">
            <!-- <label for="email1" class="form-label">Email</label> -->
            <input class="form-control" type="email" name="email" id="email1" placeholder="Email"/>
        </div>
        <div class="mb-3">
            <!-- <label for="password1" class="form-label">password</label> -->
            <input class="form-control" type="text" name="phone" id="password1" placeholder="Phone"/>
        </div>
        <div class="mb-3">
            <!-- <label for="password1" class="form-label">password</label> -->
            <input class="form-control" type="text" name="country" id="password2" placeholder="Country"/>
        </div>
        <div class="mb-3">
            <!-- <label for="password1" class="form-label">password</label> -->
            <input class="form-control" type="text" name="city" id="password2" placeholder="city"/>
        </div>
        <div class="mb-3">
            <!-- <label for="password1" class="form-label">password</label> -->
            <input class="form-control" type="text" name="address" id="password2" placeholder="address"/>
        </div>
        <div class="mb-3">
            <!-- <label for="password1" class="form-label">password</label> -->
            <input class="form-control" type="text" name="state" id="password2" placeholder="state"/>
        </div>
        <div class="mb-3">
            <!-- <label for="password1" class="form-label">password</label> -->
            <input class="form-control" type="text" name="zip" id="password2" placeholder="zip"/>
        </div>
        <div class="mb-3">
            <button class="btn btn-secondary" type="submit">Sign Up</button>

        </div>
    </form>
</div>
</div>

<%- include('footer') -%>
